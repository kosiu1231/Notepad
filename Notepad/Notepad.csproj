<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="app_logo.ico" />
    <None Remove="Components\delete.png" />
    <None Remove="img\add.png" />
    <None Remove="img\search.png" />
    <None Remove="sort.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Components\delete.png" />
    <Resource Include="img\add.png" />
    <Resource Include="img\search.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="img\app_logo.ico" />
    <Resource Include="img\sort.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
